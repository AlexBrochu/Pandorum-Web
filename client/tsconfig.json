{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"target": "es5",
		"lib": ["es5", "es6", "dom"],
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"plugins": [{"name": "typescript-tslint-plugin"}], 
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"baseUrl": "./src",
		"allowSyntheticDefaultImports": true,
		"paths": {
			"Assets/*": ["assets/*"],
			"Data/*": ["data/*"]
		}
	},
	"include":["src/**/*"]
}